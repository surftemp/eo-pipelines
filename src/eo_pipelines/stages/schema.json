{
  "id": "eo_pipelines",
  "metadata": {
    "name": "EO Pipelines",
    "version": "0.0.1",
    "description": "package for fetching and manipulating EO data"
  },
  "executor": "python",
  "dependencies": [],
  "configuration": {
    "classname": "configuration.Configuration"
  },
  "node_types": {
    "read_dataset_directory": {
      "metadata": {
        "name": "Read dataset directory",
        "description": "Read scenes from a dataset directory"
      },
      "output_ports": {
        "output": {
          "link_type": "eo_pipelines:netcdf_sceneset"
        }
      }
    },
    "read_regridded_dataset_directory": {
      "metadata": {
        "name": "Read regridded dataset directory",
        "description": "Read regridded scenes from a dataset directory"
      },
      "output_ports": {
        "output": {
          "link_type": "eo_pipelines:netcdf_regridded_sceneset"
        }
      }
    },
    "usgs_search": {
      "metadata": {
        "name": "USGS Search",
        "description": "Search for Landsat data"
      },
      "output_ports": {
        "output": {
          "link_type": "eo_pipelines:usgs_scenelist"
        }
      }
    },
    "usgs_fetch": {
      "metadata": {
        "name": "USGS Fetch",
        "description": "Fetch Landsat data"
      },
      "input_ports": {
        "input": {
          "allow_multiple_connections": false,
          "link_type": "eo_pipelines:usgs_scenelist"
        }
      },
      "output_ports": {
        "output": {
          "link_type": "eo_pipelines:raw_sceneset"
        }
      }
    },
    "landsat_import": {
      "metadata": {
        "name": "Import",
        "description": "Import USGS Landsat scenes to netcdf"
      },
      "input_ports": {
        "input": {
          "allow_multiple_connections": false,
          "link_type": "eo_pipelines:raw_sceneset"
        }
      },
      "output_ports": {
        "output": {
          "link_type": "eo_pipelines:netcdf_sceneset"
        }
      }
    },
    "xesmf_regrid": {
      "metadata": {
        "name": "Regrid",
        "description": "Regrid scenes to a target grid"
      },
      "input_ports": {
        "input": {
          "allow_multiple_connections": false,
          "link_type": "eo_pipelines:netcdf_sceneset"
        }
      },
      "output_ports": {
        "output": {
          "link_type": "eo_pipelines:netcdf_regridded_sceneset"
        }
      }
    },
    "regrid": {
      "metadata": {
        "name": "Regrid",
        "description": "Regrid scenes to a target grid"
      },
      "input_ports": {
        "input": {
          "allow_multiple_connections": false,
          "link_type": "eo_pipelines:netcdf_sceneset"
        }
      },
      "output_ports": {
        "output": {
          "link_type": "eo_pipelines:netcdf_regridded_sceneset"
        }
      }
    },
    "group": {
      "metadata": {
        "name": "Group",
        "description": "Group scenes from different datasets into one file"
      },
      "input_ports": {
        "input": {
          "allow_multiple_connections": false,
          "link_type": "eo_pipelines:netcdf_regridded_sceneset"
        }
      },
      "output_ports": {
        "output": {
          "link_type": "eo_pipelines:netcdf_regridded_sceneset"
        }
      }
    },
    "time_stacker": {
      "metadata": {
        "name": "TimeStacker",
        "description": "Append scenes on the time axis"
      },
      "input_ports": {
        "input": {
          "allow_multiple_connections": false,
          "link_type": "eo_pipelines:netcdf_regridded_sceneset"
        }
      },
      "output_ports": {
        "output": {
          "link_type": "eo_pipelines:netcdf_regridded_sceneset"
        }
      }
    },
    "rubber_stamp": {
      "metadata": {
        "name": "RubberStamp",
        "description": "Rubber Stamp data with details of this pipeline"
      },
      "input_ports": {
        "input": {
          "allow_multiple_connections": false,
          "link_type": "eo_pipelines:netcdf_regridded_sceneset"
        }
      },
      "output_ports": {
        "output": {
          "link_type": "eo_pipelines:netcdf_regridded_sceneset"
        }
      }
    },
    "missing_filter": {
      "metadata": {
        "name": "MissingFilter",
        "description": "Filter scenes to remove those with missing data"
      },
      "input_ports": {
        "input": {
          "allow_multiple_connections": false,
          "link_type": "eo_pipelines:netcdf_regridded_sceneset"
        }
      },
      "output_ports": {
        "output": {
          "link_type": "eo_pipelines:netcdf_regridded_sceneset"
        }
      }
    },
    "add_masks": {
      "metadata": {
        "name": "AddMasks",
        "description": "Add masks defined in geojson to a netcdf file"
      },
      "input_ports": {
        "input": {
          "allow_multiple_connections": false,
          "link_type": "eo_pipelines:netcdf_regridded_sceneset"
        }
      },
      "output_ports": {
        "output": {
          "link_type": "eo_pipelines:netcdf_regridded_sceneset"
        }
      }
    },
    "add_spatial": {
      "metadata": {
        "name": "AddSpatial",
        "description": "Add spatial data from netcdf file"
      },
      "input_ports": {
        "input": {
          "allow_multiple_connections": false,
          "link_type": "eo_pipelines:netcdf_regridded_sceneset"
        }
      },
      "output_ports": {
        "output": {
          "link_type": "eo_pipelines:netcdf_regridded_sceneset"
        }
      }
    },
    "custom_processor": {
      "metadata": {
        "name": "CustomProcessor",
        "description": "Perform custom processing"
      },
      "input_ports": {
        "input": {
          "allow_multiple_connections": false,
          "link_type": "eo_pipelines:netcdf_regridded_sceneset"
        }
      },
      "output_ports": {
        "output": {
          "link_type": "eo_pipelines:netcdf_regridded_sceneset"
        }
      }
    },
    "generate_html": {
      "metadata": {
        "name": "netcdf2HTML",
        "description": "Convert netcdf files to explorable HTML"
      },
      "input_ports": {
        "input": {
          "allow_multiple_connections": false,
          "link_type": "eo_pipelines:netcdf_regridded_sceneset"
        }
      },
      "output_ports": {
      }
    },
    "metadata_tweaker": {
      "metadata": {
        "name": "metadata tweaker",
        "description": "Tweak the metadata of netcdf files"
      },
      "input_ports": {
        "input": {
          "allow_multiple_connections": false,
          "link_type": "eo_pipelines:netcdf_regridded_sceneset"
        }
      },
      "output_ports": {
        "output": {
          "link_type": "eo_pipelines:netcdf_regridded_sceneset"
        }
      }
    }
  },
  "link_types": {
    "usgs_scenelist": {
      "metadata": {
        "name": "Lists of USGS scenes",
        "description": "A dictionary mapping from USGS dataset name to [catalog,dataset,scene-id]"
      }
    },
    "raw_sceneset": {
      "metadata": {
        "name": "Landsat raw scenes",
        "description": "A set of raw landsat8 scenes"
      }
    },
    "netcdf_sceneset": {
      "metadata": {
        "name": "Landsat imported scenes",
        "description": "A set of landsat scenes imported to netcdf"
      }
    },
    "netcdf_regridded_sceneset": {
      "metadata": {
        "name": "Regridded scenes",
        "description": "A set of landsat scenes imported to netcdf and regridded onto the same grid"
      }
    }
  }
}