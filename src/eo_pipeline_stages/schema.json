{
    "id": "eo_pipelines",
    "metadata": {
        "name": "EO Pipelines",
        "version": "0.0.1",
        "description": "package for fetching and manipulating EO data"
    },
    "executor": "python",
    "dependencies": [],
    "configuration": {
        "classname": "configuration.Configuration"
    },
    "node_types": {
        "sleep": {
            "metadata": {
                "name": "Sleep",
                "description": "Test stage that just... sleeps"
            },
            "classname": "nodes.sleep.Sleep"
        },
        "usgs_search": {
            "metadata": {
                "name": "USGS Search",
                "description": "Search for Landsat data"
            },
            "output_ports": {
                "output": {
                    "link_type": "eo_pipelines:usgs_scenelist"
                }
            },
            "classname": "nodes.usgs_search.USGS_Search"
        },
        "usgs_fetch": {
            "metadata": {
                "name": "USGS Fetch",
                "description": "Fetch Landsat data"
            },
            "input_ports": {
                "input": {
                    "link_type": "eo_pipelines:usgs_scenelist"
                }
            },
            "output_ports": {
                "output": {
                    "link_type": "eo_pipelines:raw_sceneset"
                }
            },
            "classname": "nodes.usgs_fetch.Fetch"
        },
        "landsat_import": {
            "metadata": {
                "name": "Import",
                "description": "Import USGS Landsat scenes to netcdf"
            },
            "input_ports": {
                "input": {
                    "link_type": "eo_pipelines:raw_sceneset"
                }
            },
            "output_ports": {
                "output": {
                    "link_type": "eo_pipelines:netcdf_sceneset"
                }
            },
            "classname": "nodes.landsat_import.LandsatImport"
        },
        "landsat_align": {
            "metadata": {
                "name": "Landsat Align",
                "description": "Align landsat scenes from a specific row/path and optionally aggregate them"
            },
            "input_ports": {
                "input": {
                    "link_type": "eo_pipelines:netcdf_sceneset"
                }
            },
            "output_ports": {
                "output": {
                    "link_type": "eo_pipelines:netcdf_sceneset"
                }
            },
            "classname": "nodes.landsat_align.LandsatAlign"
        },
        "usgs_group": {
            "metadata": {
                "name": "Group",
                "description": "Group scenes from different datasets into one file"
            },
            "input_ports": {
                "input": {
                    "link_type": "eo_pipelines:netcdf_sceneset"
                }
            },
            "output_ports": {
                "output": {
                    "link_type": "eo_pipelines:netcdf_sceneset"
                }
            },
            "classname": "nodes.usgs_group.Group"
        },
        "xesmf_regrid": {
            "metadata": {
                "name": "Regrid",
                "description": "Regrid scenes to a target grid"
            },
            "input_ports": {
                "input": {
                    "link_type": "eo_pipelines:netcdf_sceneset"
                }
            },
            "output_ports": {
                "output": {
                    "link_type": "eo_pipelines:netcdf_sceneset"
                }
            },
            "classname": "nodes.xesmf_regrid.XESMFRegrid"
        },
        "time_stacker": {
            "metadata": {
                "name": "TimeStacker",
                "description": "Append scenes on the time axis"
            },
            "input_ports": {
                "input": {
                    "link_type": "eo_pipelines:netcdf_sceneset"
                }
            },
            "output_ports": {
                "output": {
                    "link_type": "eo_pipelines:netcdf_sceneset"
                }
            },
            "classname": "nodes.time_stacker.TimeStacker"
        },
        "missing_filter": {
            "metadata": {
                "name": "MissingFilter",
                "description": "Filter scenes to remove those with missing data"
            },
            "input_ports": {
                "input": {
                    "link_type": "eo_pipelines:netcdf_sceneset"
                }
            },
            "output_ports": {
                "output": {
                    "link_type": "eo_pipelines:netcdf_sceneset"
                }
            },
            "classname": "nodes.missing_filter.MissingFilter"
        },
        "add_masks": {
            "metadata": {
                "name": "AddMasks",
                "description": "Add masks defined in geojson to a netcdf file"
            },
            "input_ports": {
                "input": {
                    "link_type": "eo_pipelines:netcdf_sceneset"
                }
            },
            "output_ports": {
                "output": {
                    "link_type": "eo_pipelines:netcdf_sceneset"
                }
            },
            "classname": "nodes.add_masks.AddMasks"
        },
        "netcdf2html": {
            "metadata": {
                "name": "netcdf2HTML",
                "description": "Convert netcdf files to explorable HTML"
            },
            "input_ports": {
                "input": {
                    "link_type": "eo_pipelines:netcdf_sceneset"
                }
            },
            "output_ports": {
            },
            "classname": "nodes.netcdf2html.Netcdf2Html"
        }
    },
    "link_types": {
        "usgs_scenelist": {
            "metadata": {
                "name": "Lists of USGS scenes",
                "description": "A dictionary mapping from USGS dataset name to [catalog,dataset,scene-id]"
            }
        },
        "raw_sceneset": {
            "metadata": {
                "name": "Landsat8 raw scenes",
                "description": "A set of raw landsat8 scenes"
            }
        },
        "netcdf_sceneset": {
            "metadata": {
                "name": "Landsat imported scenes",
                "description": "A set of landsat scenes imported to netcdf"
            }
        }
    }
}